<p id="notice"><%= notice %></p>

<h1>Listing Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Registration fname</th>
      <th>Registration lname</th>
      <th>Registration email</th>
      <th>Registration mobile</th>
      <th>Registration landline</th>
      <th>Registration state</th>
      <th>Registration district</th>
      <th>Registration taluka</th>
      <th>Registration city</th>
      <th>Registration address</th>
      <th>Registration pincode</th>
      <th>Registration username</th>
      <th>Registration password</th>
      <th>Registration type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= registration.registration_fname %></td>
        <td><%= registration.registration_lname %></td>
        <td><%= registration.registration_email %></td>
        <td><%= registration.registration_mobile %></td>
        <td><%= registration.registration_landline %></td>
        <td><%= registration.registration_state %></td>
        <td><%= registration.registration_district %></td>
        <td><%= registration.registration_taluka %></td>
        <td><%= registration.registration_city %></td>
        <td><%= registration.registration_address %></td>
        <td><%= registration.registration_pincode %></td>
        <td><%= registration.registration_username %></td>
        <td><%= registration.registration_password %></td>
        <td><%= registration.registration_type %></td>
        <td><%= link_to 'Show', registration %></td>
        <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_registration_path %>
